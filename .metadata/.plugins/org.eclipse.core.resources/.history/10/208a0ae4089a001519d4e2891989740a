package maxProfit;

public class maxProfit {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int[] cost = {5,10,20,3,40};
		findProfit(cost);
	}
	public static void findProfit(int[] cost)
	{
		int max=-999, min=999;
		for(int i=0; i<cost.length; i++)
		{
			if(max<cost[i])
				max = cost[i];
			if(min < cost[i])
				min = cost[i];
		}
	}

}
